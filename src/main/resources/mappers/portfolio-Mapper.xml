<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC
"-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="portfolioMapper">

	<select id="selectList" resultType="Constructors">
		select consid, companyname, consarea, consintroduction, profilerenameimg
		from Constructors
	</select>
	
	<select id="selectOneCons" parameterType="string" resultType="Constructors">
		select *
		from Constructors
    	where consid = #{consid}
	</select>
	
	<select id="selectRequest" parameterType="string" resultType="Constructors">
		select consid, companyname, consarea, profilerenameimg
		from Constructors
    	where consid = #{consid}
	</select>
	
	

